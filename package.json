{
  "name": "zero-vue",
  "type": "module",
  "version": "0.6.2",
  "packageManager": "pnpm@10.24.0",
  "description": "",
  "license": "MIT",
  "repository": "danielroe/zero-vue",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "prepack": "pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp",
    "test": "pnpm test:unit && pnpm test:types",
    "test:unit": "vitest",
    "test:types": "tsc --noEmit && pnpm -r test:types"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@rocicorp/zero": "catalog:deps"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@rocicorp/resolver": "catalog:test",
    "@vitest/coverage-v8": "catalog:test",
    "bumpp": "catalog:dev",
    "changelogithub": "catalog:dev",
    "eslint": "catalog:lint",
    "installed-check": "catalog:dev",
    "knip": "catalog:dev",
    "lint-staged": "catalog:lint",
    "simple-git-hooks": "catalog:dev",
    "typescript": "catalog:dev",
    "unbuild": "catalog:bundling",
    "vitest": "catalog:test",
    "vue": "catalog:peer"
  },
  "resolutions": {
    "zero-vue": "link:."
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,json,.*rc}": [
      "npx eslint --fix"
    ]
  }
}
